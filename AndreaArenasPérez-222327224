let maximunCapacityGB = 8
let defaulCapacityGB = 3
let maximunCapacityInMB = maximunCapacityGB * 1000
let defaulCapacityInMB = defaulCapacityGB * 1000
let avaliableCapacityInMB = maximunCapacityInMB - defaulCapacityInMB
let capacityRequiredMinuteVideoInMB = 150
let fullCapacityByMinutesVideo = avaliableCapacityInMB / capacityRequiredMinuteVideoInMB

