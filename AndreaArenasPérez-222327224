let weigthCratesOfOrangesInLB = 100
let weigthCratesOfWathermelonsInLB = 200
let overallWeigth = weigthCratesOfOrangesInLB + weigthCratesOfWathermelonsInLB 
let cratesOfOranges = 14
let cratesOfWathermelons = 3
let totalWeigthCratesOfOranges = weigthCratesOfOranges * cratesOfOranges
let totalWeigthCratesOfWathermelons = weigthCratesOfWathermelons * cratesOfWathermelons
let totalToLoald= totalWeigthCratesOfOranges + totalWeigthCratesOfWathermelons
let equilibrateSpaceOfTruck = totalToLoald / 2
let leftSide = equilibrateSpaceOfTruck / weigthCratesOfOranges
let rigtSide = (cratesOfOranges - leftSide) + cratesOfWathermelons
